server {
    listen 80;
    listen [::]:80;

    server_name memory2.weihanliu.space;

    location / {
        proxy_pass http://localhost:4780;
    }

    location /socket {
        proxy_pass http://localhost:4780;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";	 	 
    }
}
